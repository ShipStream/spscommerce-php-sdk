{
    "name": "shipstream/spscommerce-php-sdk",
    "description": "A PHP library for integrating with SPS Commerce via HTTP",
    "license": "MIT",
    "authors": [
        {
            "name": "Shipstream",
            "email": "info@shipstream.io"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2.1|^7.0",
        "ext-json": "*",
        "swaggest/json-schema": "^0.12"
    },
    "require-dev": {
        "php-mock/php-mock-phpunit": "^2.1"
    },
    "autoload": {
        "psr-4": {"ShipStream\\SpsCommerce\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"ShipStream\\SpsCommerce\\Tests\\": "test/"}
    },
    "scripts": {
        "test": "vendor/bin/phpunit ./test"
    }
}
